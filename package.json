{
  "name": "bktide",
  "version": "1.0.0",
  "description": "Buildkite CLI tool",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "bktide": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "NODE_OPTIONS=\"--enable-source-maps --trace-uncaught\" node build-and-run.js src/index.ts",
    "dev:interactive": "NODE_OPTIONS=\"--enable-source-maps --trace-uncaught --inspect\" node build-and-run.js src/index.ts --interactive",
    "watch": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "codegen": "./node_modules/.bin/graphql-code-generator --config codegen.mjs"
  },
  "keywords": [
    "cli",
    "buildkite"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@napi-rs/keyring": "^1.1.6",
    "chalk": "^5.5.0",
    "commander": "^13.1.0",
    "dotenv": "^16.5.0",
    "fuse.js": "^7.1.0",
    "graphql": "^16.10.0",
    "graphql-request": "^6.1.0",
    "html-to-text": "^9.0.5",
    "node-fetch": "^3.3.2",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.0",
    "prompts": "^2.4.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^6.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@parcel/watcher": "^2.4.1",
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^22.14.1",
    "@types/node-fetch": "^2.6.12",
    "@types/node-persist": "^3.1.8",
    "@types/prompts": "^2.4.9",
    "@types/source-map-support": "^0.5.10",
    "node-persist": "^4.0.4",
    "tsc-files": "^1.1.4"
  }
}
