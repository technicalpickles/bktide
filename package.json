{
  "name": "bk-cli",
  "version": "1.0.0",
  "description": "Buildkite CLI tool",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "bk-cli": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "node --import=./register-ts-node.js src/index.ts",
    "dev:alt": "node --no-warnings --loader ts-node/esm src/index.ts",
    "watch": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cli",
    "buildkite"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^13.1.0",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.1"
  }
}
