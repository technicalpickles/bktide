{
  "name": "bk-cli",
  "version": "1.0.0",
  "description": "Buildkite CLI tool",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "bk-cli": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "node --import=./register-ts-node.js src/index.ts",
    "dev:alt": "node --no-warnings --loader ts-node/esm src/index.ts",
    "dev:sourcemap": "node --enable-source-maps --import=./register-ts-node.js src/index.ts",
    "dev:enhanced": "NODE_OPTIONS=\"--enable-source-maps --trace-uncaught\" node --import=./register-ts-node.js src/index.ts",
    "dev:debug": "NODE_DEBUG=module,esm,loader node --enable-source-maps --trace-uncaught --import=./register-ts-node.js src/index.ts",
    "dev:runner": "node run-with-sourcemaps.js src/index.ts",
    "dev:compiled": "node build-and-run.js src/index.ts",
    "watch": "tsc --watch",
    "debug": "node --inspect --import=./register-ts-node.js src/index.ts",
    "debug:inspect": "node --inspect --enable-source-maps --import=./register-ts-node.js src/index.ts",
    "dev:trace": "node run-with-stack-traces.js src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "codegen": "./node_modules/.bin/graphql-code-generator --config codegen.mjs && node fix-imports.js"
  },
  "keywords": [
    "cli",
    "buildkite"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^13.1.0",
    "dotenv": "^16.5.0",
    "fuse.js": "^7.1.0",
    "graphql": "^16.10.0",
    "graphql-request": "^6.1.0",
    "node-fetch": "^3.3.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^6.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@parcel/watcher": "^2.4.1",
    "@types/node": "^22.14.1",
    "@types/node-fetch": "^2.6.12",
    "@types/node-persist": "^3.1.8",
    "@types/source-map-support": "^0.5.10",
    "node-persist": "^4.0.4"
  }
}
